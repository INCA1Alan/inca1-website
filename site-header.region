<!doctype html>
<html>
    <head>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="viewport" content="initial-scale=1">

        <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,700,300,600' rel='stylesheet' type='text/css'>

        <!--Include sqs scripts, meta tags, and user content from the code injection tab.-->
        {squarespace-headers}
    </head>
    <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
        <header class="header">
            {.section website}
                <a class="header__logo logo logo--large" href="/">
                    <img src="/assets/inca.svg" alt="{siteTitle}">
                </a>
                <a class="header__logo logo logo--small" href="/">
                    <img src="/assets/inca-sm.svg" alt="{siteTitle}">
                </a>
                <div class="header__content">
                    {.section siteTagLine}
                        <p>{@}</p>
                    {.end}
                    <nav class="header-nav">
                        <button type="button" class="menu__open">Menu</button>
                    </nav>
                </div>
            {.end}
        </header>
        <squarespace:navigation navigationId="mainNav" template="navigation">
        <div class="body">
