<nav class="subnav">
  <ul>
    {.repeated section items}
    <li class="subnav__item {.section active} subnav__item--active{.end}">
    {@}

      {.section collection}
        <a href="{fullUrl}">
            <i>{navigationTitle}</i>
            <span>{title}</span>
        </a>
      {.end}

      {.section externalLink}
        <a href="{url}"{.section newWindow} target="_blank"{.end}>{title}</a>
      {.end}

    </li>
    {.end}
  </ul>
</nav>
