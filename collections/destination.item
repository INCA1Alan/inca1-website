<section>
    <p>main content comes next</p>
    {squarespace.main-content}
</section>

{.section collection}
<section>
    <p>collection meta-content comes next</p>
    <h1>{title}</h1>
</section>
{.end}

{.repeated section items}
    <article>
        <h1>{title}</h1>
        <p>{description}</p>
    </article>
{.end}

<section>
    <p>items content comes next</p>
    {.repeated section items}
        <p>{id}</p>
        <p>{body}</p>
    {.or}
        <p>no items!</p>
    {.end}
</section>

<aside>
    <p>some blocks here</p>
    <squarespace:block-field id="someBlocks" />
</aside>
